{
	"name": "dragon-court",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"prebuild:server": "rimraf dist",
		"build:server": "babel ./src/server --out-dir dist/server",
		"prebuild:client:site": "rimraf public/assets/js/site.js; rimraf public/assets/js/site.js.map",
		"build:client:site": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c site.rollup.config.js",
		"prebuild:client:game": "rimraf public/assets/js/entry.js",
		"build:client:game": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c game.rollup.config.js",
		"build:client": "npm-run-all build:client:site build:client:game",
		"prebuild": "mysql -e 'source database.sql'",
		"build": "npm-run-all build:server build:client run:dev",
		"run:dev": "fuser -k 33039/tcp; nodemon dist/server/index.js"
	},
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"bootstrap": "^5.1.3",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"express-handlebars": "^6.0.5",
		"express-mysql-session": "^2.1.7",
		"express-session": "^1.17.2",
		"helmet": "^5.1.0",
		"jquery": "^3.6.0",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.3",
		"mysql2": "^2.3.3",
		"nodemailer": "^6.7.5",
		"nodemon": "^2.0.16",
		"npm-run-all": "^4.1.5",
		"phaser": "^3.55.2",
		"socket.io": "^4.4.1",
		"socket.io-client": "^4.5.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.18.2",
		"@babel/eslint-parser": "^7.18.2",
		"@babel/preset-env": "^7.18.2",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-replace": "^4.0.0",
		"@types/bootstrap": "^5.1.12",
		"@types/jquery": "^3.5.14",
		"@types/node": "^18.0.0",
		"@types/react": "^18.0.14",
		"@types/react-bootstrap": "^0.32.30",
		"@types/react-dom": "^18.0.5",
		"@typescript-eslint/eslint-plugin": "^5.28.0",
		"@typescript-eslint/parser": "^5.28.0",
		"eslint": "^8.18.0",
		"eslint-plugin-react": "^7.30.0",
		"prettier": "^2.7.1",
		"react": "^18.2.0",
		"react-bootstrap": "^2.4.0",
		"react-dom": "^18.2.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.75.6",
		"rollup-plugin-styles": "^4.0.0",
		"rollup-plugin-typescript2": "^0.32.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	}
